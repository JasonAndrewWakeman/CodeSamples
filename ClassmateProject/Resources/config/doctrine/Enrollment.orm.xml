<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Project\ClassMateBundle\Entity\Enrollment" table="enrollment">
    <id name="enrollmentId" type="integer" column="enrollment_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="role" type="string" column="role" length="1" nullable="true"/>
    <field name="courseId" type="integer" column="course_id" length="11" nullable="true"/>
    <field name="courseName" type="string" column="course_name" length="45" nullable="true"/>
    <field name="enrolled" type="string" column="enrolled" length="1" nullable="true"/>
    <field name="userId" type="integer" column="user_id" nullable="true"/>
  </entity>
</doctrine-mapping>
